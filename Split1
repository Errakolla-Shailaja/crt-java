import java.util.*;
class Split1{
public static void main(String[] args){
Scanner s=new Scanner(System.in);
String str=s.nextLine();
String res[]=str.split(" ");
int max=0;
String s1=" ";
for(int i=0;i<res.length;i++)
{
int len=res[i].length();
if(max<len){
s1=res[i];
}
}
System.out.println(s1);
}
}
